<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:target="http://www.masicengine.ru"
           targetNamespace="http://www.masicengine.ru"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">

    <xs:complexType name="ColorAsAttrComplexType">
        <xs:attribute name="r" type="xs:float"></xs:attribute>
        <xs:attribute name="g" type="xs:float"></xs:attribute>
        <xs:attribute name="b" type="xs:float"></xs:attribute>
    </xs:complexType>

    <xs:complexType name="Vec3ComplexType">
        <xs:attribute name="x" type="xs:float"></xs:attribute>
        <xs:attribute name="y" type="xs:float"></xs:attribute>
        <xs:attribute name="z" type="xs:float"></xs:attribute>
    </xs:complexType>

    <xs:simpleType name="FilterSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="nearest"></xs:enumeration>
            <xs:enumeration value="linear"></xs:enumeration>
            <xs:enumeration value="cubic_ext"></xs:enumeration>
            <xs:enumeration value="cubic_img"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SamplerAddressModeSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="repeat"></xs:enumeration>
            <xs:enumeration value="mirrored_repeat"></xs:enumeration>
            <xs:enumeration value="clamp_to_edge"></xs:enumeration>
            <xs:enumeration value="clamp_to_border"></xs:enumeration>
            <xs:enumeration value="mirror_clamp_to_edge"></xs:enumeration>
            <xs:enumeration value="mirror_clamp_to_edge_khr"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CompareOpSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="never"></xs:enumeration>
            <xs:enumeration value="less"></xs:enumeration>
            <xs:enumeration value="equal"></xs:enumeration>
            <xs:enumeration value="less_or_equal"></xs:enumeration>
            <xs:enumeration value="greater"></xs:enumeration>
            <xs:enumeration value="not_equal"></xs:enumeration>
            <xs:enumeration value="greater_or_equal"></xs:enumeration>
            <xs:enumeration value="always"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BorderColorSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="float_transparent_black"></xs:enumeration>
            <xs:enumeration value="int_transparent_black"></xs:enumeration>
            <xs:enumeration value="float_opaque_black"></xs:enumeration>
            <xs:enumeration value="int_opaque_black"></xs:enumeration>
            <xs:enumeration value="float_opaque_white"></xs:enumeration>
            <xs:enumeration value="int_opaque_white"></xs:enumeration>
            <xs:enumeration value="float_custom_ext"></xs:enumeration>
            <xs:enumeration value="int_custom_ext"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="SamplerComplexType">
        <xs:sequence>
            <xs:element name="CreateFlags" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Flags">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="subsampled_bit_ext"></xs:enumeration>
                                    <xs:enumeration value="subsampled_coarse_reconstruction_bit_ext"></xs:enumeration>
                                    <xs:enumeration value="descriptor_buffer_capture_replay_bit_ext"></xs:enumeration>
                                    <xs:enumeration value="non_seamless_cube_map_bit_ext"></xs:enumeration>
                                    <xs:enumeration value="image_processing_bit_qcom"></xs:enumeration>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="MagFilter" type="target:FilterSimpleType"></xs:element>
            <xs:element name="MinFilter" type="target:FilterSimpleType"></xs:element>
            <xs:element name="MipmapMode">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="nearest"></xs:enumeration>
                        <xs:enumeration value="linear"></xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="AddressModeU" type="target:SamplerAddressModeSimpleType"></xs:element>
            <xs:element name="AddressModeV" type="target:SamplerAddressModeSimpleType"></xs:element>
            <xs:element name="AddressModeW" type="target:SamplerAddressModeSimpleType"></xs:element>
            <xs:element name="MipLodBias" type="xs:float"></xs:element>
            <xs:element name="AnisotropyEnable" type="xs:boolean"></xs:element>
            <xs:element name="MaxAnisotropy" type="xs:float"></xs:element>
            <xs:element name="CompareEnable" type="xs:boolean"></xs:element>
            <xs:element name="CompareOp" type="target:CompareOpSimpleType"></xs:element>
            <xs:element name="MinLod" type="xs:float"></xs:element>
            <xs:element name="MaxLod" type="xs:float"></xs:element>
            <xs:element name="BorderColor" type="target:BorderColorSimpleType"></xs:element>
            <xs:element name="UnnormalizedCoordinates" type="xs:boolean"></xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ShaderStageFlagBitsComplexType">
        <xs:sequence>
            <xs:element name="Flag" minOccurs="0" maxOccurs="unbounded">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="vertex_bit"></xs:enumeration>
                        <xs:enumeration value="tessellation_control_bit"></xs:enumeration>
                        <xs:enumeration value="tessellation_evaluation_bit"></xs:enumeration>
                        <xs:enumeration value="geometry_bit"></xs:enumeration>
                        <xs:enumeration value="fragment_bit"></xs:enumeration>
                        <xs:enumeration value="compute_bit"></xs:enumeration>
                        <xs:enumeration value="all_graphics"></xs:enumeration>
                        <xs:enumeration value="all"></xs:enumeration>
                        <xs:enumeration value="raygen_bit_khr"></xs:enumeration>
                        <xs:enumeration value="any_hit_bit_khr"></xs:enumeration>
                        <xs:enumeration value="closest_hit_bit_khr"></xs:enumeration>
                        <xs:enumeration value="miss_bit_khr"></xs:enumeration>
                        <xs:enumeration value="intersection_bit_khr"></xs:enumeration>
                        <xs:enumeration value="callable_bit_khr"></xs:enumeration>
                        <xs:enumeration value="task_bit_ext"></xs:enumeration>
                        <xs:enumeration value="mesh_bit_ext"></xs:enumeration>
                        <xs:enumeration value="subpass_shading_bit_huawei"></xs:enumeration>
                        <xs:enumeration value="cluster_culling_bit_huawei"></xs:enumeration>
                        <xs:enumeration value="raygen_bit_nv"></xs:enumeration>
                        <xs:enumeration value="any_hit_bit_nv"></xs:enumeration>
                        <xs:enumeration value="closest_hit_bit_nv"></xs:enumeration>
                        <xs:enumeration value="miss_bit_nv"></xs:enumeration>
                        <xs:enumeration value="intersection_bit_nv"></xs:enumeration>
                        <xs:enumeration value="callable_bit_nv"></xs:enumeration>
                        <xs:enumeration value="task_bit_nv"></xs:enumeration>
                        <xs:enumeration value="mesh_bit_nv"></xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LayoutBindingComplexType">
        <xs:all>
            <xs:element name="Binding" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>specified in shader - layout (set=m, binding=n)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DescriptorType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="sampler"></xs:enumeration>
                        <xs:enumeration value="combined_image_sampler"></xs:enumeration>
                        <xs:enumeration value="sampled_image"></xs:enumeration>
                        <xs:enumeration value="storage_image"></xs:enumeration>
                        <xs:enumeration value="uniform_texel_buffer"></xs:enumeration>
                        <xs:enumeration value="storage_texel_buffer"></xs:enumeration>
                        <xs:enumeration value="uniform_buffer"></xs:enumeration>
                        <xs:enumeration value="storage_buffer"></xs:enumeration>
                        <xs:enumeration value="uniform_buffer_dynamic"></xs:enumeration>
                        <xs:enumeration value="storage_buffer_dynamic"></xs:enumeration>
                        <xs:enumeration value="input_attachment"></xs:enumeration>
                        <xs:enumeration value="inline_uniform_block"></xs:enumeration>
                        <xs:enumeration value="acceleration_structure_khr"></xs:enumeration>
                        <xs:enumeration value="acceleration_structure_nv"></xs:enumeration>
                        <xs:enumeration value="sample_weight_image_qcom"></xs:enumeration>
                        <xs:enumeration value="block_match_image_qcom"></xs:enumeration>
                        <xs:enumeration value="mutable_ext"></xs:enumeration>
                        <xs:enumeration value="inline_uniform_block_ext"></xs:enumeration>
                        <xs:enumeration value="mutable_valve"></xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="DescriptorCount" type="xs:unsignedInt"></xs:element>
            <xs:element name="ShaderStageFlags" type="target:ShaderStageFlagBitsComplexType"></xs:element>
            <xs:element name="ImmutableSamplers" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Sampler" type="target:SamplerComplexType" minOccurs="0" maxOccurs="unbounded"></xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:all>
    </xs:complexType>

    <xs:simpleType name="ShaderStageSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="vertex_bit"></xs:enumeration>
            <xs:enumeration value="tessellation_control_bit"></xs:enumeration>
            <xs:enumeration value="tessellation_evaluation_bit"></xs:enumeration>
            <xs:enumeration value="geometry_bit"></xs:enumeration>
            <xs:enumeration value="fragment_bit"></xs:enumeration>
            <xs:enumeration value="compute_bit"></xs:enumeration>
            <xs:enumeration value="all_graphics"></xs:enumeration>
            <xs:enumeration value="all"></xs:enumeration>
            <xs:enumeration value="raygen_bit_khr"></xs:enumeration>
            <xs:enumeration value="any_hit_bit_khr"></xs:enumeration>
            <xs:enumeration value="closest_hit_bit_khr"></xs:enumeration>
            <xs:enumeration value="miss_bit_khr"></xs:enumeration>
            <xs:enumeration value="intersection_bit_khr"></xs:enumeration>
            <xs:enumeration value="callable_bit_khr"></xs:enumeration>
            <xs:enumeration value="task_bit_ext"></xs:enumeration>
            <xs:enumeration value="mesh_bit_ext"></xs:enumeration>
            <xs:enumeration value="subpass_shading_bit_huawei"></xs:enumeration>
            <xs:enumeration value="cluster_culling_bit_huawei"></xs:enumeration>
            <xs:enumeration value="raygen_bit_nv"></xs:enumeration>
            <xs:enumeration value="any_hit_bit_nv"></xs:enumeration>
            <xs:enumeration value="closest_hit_bit_nv"></xs:enumeration>
            <xs:enumeration value="miss_bit_nv"></xs:enumeration>
            <xs:enumeration value="intersection_bit_nv"></xs:enumeration>
            <xs:enumeration value="callable_bit_nv"></xs:enumeration>
            <xs:enumeration value="task_bit_nv"></xs:enumeration>
            <xs:enumeration value="mesh_bit_nv"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="GLSLTypesSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="bool"></xs:enumeration>
            <xs:enumeration value="int"></xs:enumeration>
            <xs:enumeration value="uint"></xs:enumeration>
            <xs:enumeration value="float"></xs:enumeration>
            <xs:enumeration value="double"></xs:enumeration>
            <xs:enumeration value="bvec2"></xs:enumeration>
            <xs:enumeration value="bvec3"></xs:enumeration>
            <xs:enumeration value="bvec4"></xs:enumeration>
            <xs:enumeration value="ivec2"></xs:enumeration>
            <xs:enumeration value="ivec3"></xs:enumeration>
            <xs:enumeration value="ivec4"></xs:enumeration>
            <xs:enumeration value="uvec2"></xs:enumeration>
            <xs:enumeration value="uvec3"></xs:enumeration>
            <xs:enumeration value="uvec4"></xs:enumeration>
            <xs:enumeration value="vec2"></xs:enumeration>
            <xs:enumeration value="vec3"></xs:enumeration>
            <xs:enumeration value="vec4"></xs:enumeration>
            <xs:enumeration value="dvec2"></xs:enumeration>
            <xs:enumeration value="dvec3"></xs:enumeration>
            <xs:enumeration value="dvec4"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="SpecializationConstantComplexType">
        <xs:sequence>
            <xs:element name="Constant" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ConstantID" type="xs:unsignedInt"></xs:element>
                        <xs:element name="Type" type="target:GLSLTypesSimpleType"></xs:element>
                        <xs:element name="Value"></xs:element>
                    </xs:sequence>
                    <xs:attribute name="name" type="xs:string"></xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="ShaderFlagSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="link_stage_bit_ext"></xs:enumeration>
            <xs:enumeration value="allow_varying_subgroup_size"></xs:enumeration>
            <xs:enumeration value="require_full_subgroups"></xs:enumeration>
            <xs:enumeration value="no_task_shader_bit_ext"></xs:enumeration>
            <xs:enumeration value="dispatch_base_bit_ext"></xs:enumeration>
            <xs:enumeration value="fragment_shading_rate_attachment_bit_ext"></xs:enumeration>
            <xs:enumeration value="fragment_density_map_attachment_bit_ext"></xs:enumeration>
            <xs:enumeration value="indirect_bindable_bit_ext"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="ShaderFlagsComplexType">
        <xs:sequence>
            <xs:element name="Flag" type="target:ShaderFlagSimpleType" minOccurs="1" maxOccurs="unbounded"></xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ShaderStageFlagsComplexType">
        <xs:sequence>
            <xs:element name="Flag" type="target:ShaderStageSimpleType" minOccurs="1" maxOccurs="unbounded"></xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ShaderComplexType">
        <xs:sequence>
            <xs:element name="FilePath" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
            <xs:element name="Flags" type="target:ShaderFlagsComplexType" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element name="Stage" type="target:ShaderStageFlagsComplexType" minOccurs="1" maxOccurs="1"></xs:element>
            <xs:element name="SpecializationConstants" type="target:SpecializationConstantComplexType" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element name="InputAttributeDescription" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Attribute" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Location" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"></xs:element>
                                    <xs:element name="Format" type="target:GLSLTypesSimpleType" minOccurs="1" maxOccurs="1"></xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string"></xs:attribute>
    </xs:complexType>

    <xs:simpleType name="InputAssemblySimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="point_list"></xs:enumeration>
            <xs:enumeration value="line_list"></xs:enumeration>
            <xs:enumeration value="line_strip"></xs:enumeration>
            <xs:enumeration value="triangle_list"></xs:enumeration>
            <xs:enumeration value="triangle_strip"></xs:enumeration>
            <xs:enumeration value="triangle_fan"></xs:enumeration>
            <xs:enumeration value="line_list_with_adjacency"></xs:enumeration>
            <xs:enumeration value="line_strip_with_adjacency"></xs:enumeration>
            <xs:enumeration value="triangle_list_with_adjacency"></xs:enumeration>
            <xs:enumeration value="triangle_strip_with_adjacency"></xs:enumeration>
            <xs:enumeration value="patch_list"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PolygonModeSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="fill"></xs:enumeration>
            <xs:enumeration value="line"></xs:enumeration>
            <xs:enumeration value="point"></xs:enumeration>
            <xs:enumeration value="fill_rectangle_nv"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CullModeFlagSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="none"></xs:enumeration>
            <xs:enumeration value="front_bit"></xs:enumeration>
            <xs:enumeration value="back_bit"></xs:enumeration>
            <xs:enumeration value="front_and_back"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="FrontFaceSimpleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="counter_clockwise"></xs:enumeration>
            <xs:enumeration value="clockwise"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
        
    <xs:complexType name="GraphicsPipelineComplexType">
        <xs:sequence>
            <xs:element name="DescriptorSet" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Set" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="LayoutBinding" type="target:LayoutBindingComplexType" minOccurs="1" maxOccurs="unbounded"></xs:element>
                                </xs:sequence>
                                <xs:attribute name="slot" type="xs:unsignedInt"></xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Shaders" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Shader" type="target:ShaderComplexType" minOccurs="1" maxOccurs="unbounded"></xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="InputAssembly" type="target:InputAssemblySimpleType" minOccurs="1" maxOccurs="1"></xs:element>
            <xs:element name="RasterizationState" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="DepthClampEnable" type="xs:boolean" minOccurs="1" maxOccurs="1"></xs:element>
                        <xs:element name="RasterizerDiscardEnable" type="xs:boolean" minOccurs="1" maxOccurs="1"></xs:element>
                        <xs:element name="PolygonMode" type="target:PolygonModeSimpleType" minOccurs="1" maxOccurs="1"></xs:element>
                        <xs:element name="CullMode" minOccurs="1" maxOccurs="1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Flags" minOccurs="1" maxOccurs="1">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Flag" type="target:CullModeFlagSimpleType" minOccurs="1" maxOccurs="unbounded"></xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="FrontFace" type="target:FrontFaceSimpleType"></xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:complexType name="PipelinesComplexType">
        <xs:sequence>
          <xs:element name="GraphicsPipeline" type="target:GraphicsPipelineComplexType" minOccurs="0" maxOccurs="unbounded"></xs:element>
          <xs:element name="ComputePipeline" minOccurs="0" maxOccurs="unbounded"></xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="Pipelines" type="target:PipelinesComplexType"></xs:element>
    <xs:element name="World">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Actors" minOccurs="0" maxOccurs="1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Actor" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="TransformComponent" minOccurs="0" maxOccurs="1">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="Position" type="target:Vec3ComplexType"></xs:element>
                                                    <xs:element name="YawPitchRoll" type="target:Vec3ComplexType"></xs:element>
                                                    <xs:element name="Scale" type="target:Vec3ComplexType"></xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="CameraComponent">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="Fov" type="xs:float"></xs:element>
                                                    <xs:element name="Near" type="xs:float"></xs:element>
                                                    <xs:element name="Far" type="xs:float"></xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="ModelComponent">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="FilePath" type="xs:string"></xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="name" type="xs:string"></xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Scene">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="BackgroundColor" type="target:ColorAsAttrComplexType"></xs:element>
                            <xs:element name="Camera">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="SelectName" type="xs:string"></xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="Fog">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="FogStart" type="xs:float"></xs:element>
                                        <xs:element name="FogRange" type="xs:float"></xs:element>
                                        <xs:element name="FogColor" type="target:ColorAsAttrComplexType"></xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="ActorAnimations">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="ActorAnimation" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="AnimationForActorName" type="xs:string"></xs:element>
                                                    <xs:element name="RelativeKeyframe">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="TimePosSec" type="xs:float"></xs:element>
                                                                <xs:element name="YawPitchRoll" type="target:Vec3ComplexType"></xs:element>
                                                                <xs:element name="Translation" type="target:Vec3ComplexType"></xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="TotalTimeSec" type="xs:float"></xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="PlayerOptions"></xs:element>

</xs:schema>